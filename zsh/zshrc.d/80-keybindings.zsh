#!/bin/zsh

# Vim Mode
bindkey -v


# This is terminal specific
bindkey "^[[H"     beginning-of-line     # Home
bindkey "^[[4~"    end-of-line           # End
bindkey "^[[P"     delete-char           # Delete
bindkey "^[[5~"    backward-word         # PageUp/Prior
bindkey "^[[6~"    forward-word          # PageDown/Next

