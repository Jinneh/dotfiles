prepopulate-query-feeds yes

auto-reload yes
refresh-on-startup yes
reload-time 60
max-items 60

notify-program "notify-send"
notify-format "newsboat '%d new articles (%n unread)'"

datetime-format "%d %m %y, %R"

bind-key m toggle-show-read-feeds

browser $BROWSER
